<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.douzone.inspectionservice.mapper.InspectionAddMapper">
    <insert id="InspectionAdd" parameterType="Map">
        INSERT into register
        (inspector_id,barcode,register_dt)
        values
        (#{inspectorId},#{barcode},now());
    </insert>
</mapper>